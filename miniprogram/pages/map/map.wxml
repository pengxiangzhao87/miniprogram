<view class="container">

  <map id="myMap" style="width: 100%; height: 300px;" longitude="{{longitude}}" latitude="{{latitude}}" markers="{{markers}}" covers="{{covers}}" show-location></map>

  <cover-view class="cover-view">
    <mp-searchbar bindselectresult="selectResult" search="{{search}}"></mp-searchbar>
  </cover-view>
<view class="container">
<text>{{address}}</text>
<text>{{name}}</text>
<text>{{number}}</text>
<text>{{content}}</text>
</view>
  
</view>